{
  "name": "meet-shop-backend-engineer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run swagger && npx tsc && npm run copy-swagger-ui",
    "dev": "concurrently \"nodemon -q\" \"nodemon -q --config nodemon-swagger.json\"",
    "swagger": "tsoa spec-and-routes",
    "copy-swagger-ui": "cp -r ./node_modules/swagger-ui-dist ./dist/src/swagger-ui-dist",
    "eslint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npx eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit\\.spec\\.ts",
    "test:int": "jest --testPathPattern=int\\.spec\\.ts",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.9.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
